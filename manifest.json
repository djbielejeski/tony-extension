{
  "manifest_version": 2,
  "name": "Script Runner",
  "description": "Runs some code when the page changes.",
  "version": "1.0",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "background": { "page": "checkWebPage.html" },
  "permissions": ["tabs"]
}
